	private String id;
	private static int soId;
	private static Random random;
	
	
	public ManChoi(String nguoiChoi) {
		this.id = "GAME" + String.format("%05d", soId++);
	}
	
	private int getRandomInt() {
		Random r = new Random();
	}
}
